<h1>Editing Diagnostic Assessment</h1>

<% form_for(@unit_planner) do |f| %>
  <%= f.error_messages %>

<%=hidden_field_tag :anchor, "diagnostic_assessment"%>

  <p>
    <%= f.label :diagnostic_assessments %><br />
    <%= f.text_area :diagnostic_assessments %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Back', unit_planner_path(@unit_planner, :anchor=>"prior_knowledge") %>