<h1>New learning_style</h1>

<% form_for(@learning_style) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :intelligence_id %><br />
    <%= f.text_field :intelligence_id %>
  </p>
  <p>
    <%= f.label :unit_planner_id %><br />
    <%= f.text_field :unit_planner_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', learning_styles_path %>