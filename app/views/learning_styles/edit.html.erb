<h1>Editing learning_style</h1>

<% form_for([@unit_planner, @learning_style]) do |f| %>
  <%= f.error_messages %>

<%= render :partial => 'form', :object=>f %>

  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Back to Unit Planner', unit_planner_path(@unit_planner, :anchor=>'learning_styles') %>